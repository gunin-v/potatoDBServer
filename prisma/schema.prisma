datasource db {
  provider = "sqlite"
  url      = "file:./dbs/dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id            Int      @id @default(autoincrement())
  name          String
  price         Float
  discountPrice Float?
  store         String
  updated_at    DateTime @default(now())
}